alias vi="nvim"
alias code="codium"
alias aliaslist="cat $HOME/.config/zsh/aliases"

# Git
alias gs="git status"
alias ga="git add"
alias gall="git add ."
alias gc="git commit -m"
alias gpush="git push"
alias gpull="git pull"
alias glog="git log --oneline"
alias glogl="git log -1 --oneline"
alias gsw="git switch"
alias gswc="git switch -c"
alias gdiff="git diff"
alias gi="git init"

# Docker compose
alias dc=docker-compose
